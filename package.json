{
  "name": "mov-to-mp4-lambda",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "rm -rf {build,dist} && mkdir dist",
    "compile": "babel src -d dist",
    "deploy": "npm run clean && npm run compile && node-lambda deploy -e production",
    "lint": "eslint --config .eslintrc.js src test",
    "local": "npm run compile && node-lambda run",
    "package": "npm run clean && npm run compile && node-lambda package -e production",
    "postinstall": "node-lambda setup",
    "test": "mocha --compilers js:babel-core/register --reporter mocha-better-spec-reporter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrdziuban/mov-to-mp4-lambda.git"
  },
  "author": "Matt Dziuban",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.95.0",
    "ffmpeg-lambda-binary": "github:mrdziuban/ffmpeg-lambda-binary#master",
    "fs-extra": "^4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.4.2",
    "mocha-better-spec-reporter": "^3.1.0",
    "node-lambda": "^0.11.3",
    "sinon": "^3.1.0",
    "sinon-chai": "^2.12.0"
  }
}
